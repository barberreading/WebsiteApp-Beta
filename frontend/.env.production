# Production Environment Configuration for Frontend
# This file contains production environment variables

# API Configuration
REACT_APP_API_URL=https://your-api-domain.com/api
REACT_APP_API_VERSION=v1

# Application Configuration
REACT_APP_NAME=Staff Management System
REACT_APP_VERSION=1.2.0
REACT_APP_ENVIRONMENT=production

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Performance Configuration
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# Security Configuration
REACT_APP_ENABLE_HTTPS=true
REACT_APP_SECURE_COOKIES=true

# Feature Flags
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_CONSOLE_LOGS=false

# CDN Configuration (if using)
# REACT_APP_CDN_URL=https://your-cdn-domain.com

# Third-party Services (if using)
# REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# REACT_APP_SENTRY_DSN=https://your-sentry-dsn

# Cache Configuration
REACT_APP_CACHE_VERSION=1.2.0
REACT_APP_ENABLE_SERVICE_WORKER=true

# UI Configuration
REACT_APP_THEME=default
REACT_APP_BRAND_COLOR=#007bff

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Session Configuration
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_IDLE_TIMEOUT=1800000